{
	"name": "not-pretty",
	"version": "0.0.5",
	"description": "not-* family module for static pages pretty urls in not- environment",
	"main": "index.js",
	"scripts": {
		"test": "./node_modules/.bin/mocha --reporter spec",
		"lint": "./node_modules/.bin/eslint ./src/**/**",
		"docs": "./node_modules/.bin/jsdoc -c jsdoc.json",
		"cover": "istanbul cover _mocha $(find ./test -name \"*.js\" -not -path \"./node_modules/*\")"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/interrupter/not-pretty.git"
	},
	"keywords": [
		"not-framework",
		"not-node",
		"pretty"
	],
	"author": "interrupter <alex.repin@mail.ru> (http://github.com/interrupter)",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/interrupter/not-pretty/issues"
	},
	"homepage": "https://github.com/interrupter/not-pretty#readme",
	"dependencies": {
		"not-config": "*",
		"serve-static": "^1.13.1"
	},
	"devDependencies": {
		"chai": "*",
		"chai-as-promised": "*",
		"eslint": "*",
		"ink-docstrap": "^1.3.2",
		"istanbul": "*",
		"jsdoc": "^3.5.5",
		"mocha": "*"
	}
}
